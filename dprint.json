{
	"typescript": {
		"lineWidth": 100,
		"indentWidth": 4,
		"useTabs": true,
		"quoteProps": "asNeeded"
	},
	"json": {
		"lineWidth": 100,
		"indentWidth": 4,
		"useTabs": true
	},
	"markdown": {},
	"malva": {
		"indentWidth": 4,
		"useTabs": true
	},
	"excludes": [
		"**/node_modules",
		".next",
		"next-env.d.ts"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.94.0.wasm",
		"https://plugins.dprint.dev/markdown-0.18.0.wasm",
		"https://plugins.dprint.dev/g-plane/malva-v0.11.2.wasm",
		"https://plugins.dprint.dev/json-0.20.0.wasm"
	]
}
