		"@types/react-dom": "19.2.3",
		"@types/simple-peer": "9.11.9",
		"babel-plugin-react-compiler": "1.0.0",
		"typescript": "5.9.3"
	},
	"browserslist": [
		"defaults and fully supports es6-module",
		"maintained node versions"
